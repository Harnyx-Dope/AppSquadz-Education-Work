package AppsquaqzEx1;

import java.util.Scanner;

public class Pattern1
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        {
            for (int i = 1; i <=5; i++) {
                var t=4+i;
                for (int j = t; j >= i; j--) {
                        System.out.print(j);
                }
                System.out.println();
            }
        }//Local block
    }
}
