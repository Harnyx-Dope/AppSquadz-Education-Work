package AppsquaqzEx1.Numbers;

import java.util.Scanner;

public class BuzzNo
{
    static  int n;
    static
    {
        n=new Scanner(System.in).nextInt();
    }
    public static void main(String[] args)
    {
        if(n%10==7 || n%7==0 && n!=0)
            System.out.println("Number is Buzz Number");
        else
            System.out.println("Not An Buzz Number");

    }
}
