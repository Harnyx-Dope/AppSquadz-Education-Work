package AppsquaqzEx1;

import java.util.Scanner;

public class Pattern1
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        {
            var f = 1;
            for (int i = 1; i <= 5; i++) {
                var len=4+f++;

                for (int j = i; j <=  len; j++) {
                    System.out.print(j);
                }
                System.out.println();
 
            }
        }//Local block
    }
}
